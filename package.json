    "@unocss/transformer-directives": "^0.47.5",
    "@unocss/vite": "^0.47.5",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "czg": "^1.4.0",
    "electron": "^22.0.0",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.29.0",
